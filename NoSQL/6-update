// Script avec cde qui met Ã  jour tous les documents de la collection school dont le champ name vaut "Holberton school"
db.schoool.update(
    {
        "name": "Holberton school"
    },
    {
        $set: { 
            "address": "972 Mission street"
        }
    },
    {
        multi: true 
    }
)
